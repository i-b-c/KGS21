{
  "kind": "collectionType",
  "collectionName": "performances",
  "info": {
    "name": "Performance",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name_et": {
      "type": "string"
    },
    "name_en": {
      "type": "string"
    },
    "slug_et": {
      "type": "uid"
    },
    "slug_en": {
      "type": "uid",
      "targetField": "subtitle_en"
    },
    "subtitle_et": {
      "type": "string"
    },
    "subtitle_en": {
      "type": "string"
    },
    "categories": {
      "collection": "category"
    },
    "coproduction": {
      "type": "boolean"
    },
    "coproduction_importance": {
      "type": "decimal"
    },
    "front_page_promotion": {
      "type": "boolean"
    },
    "logos": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "purchase_description_et": {
      "type": "text"
    },
    "description_et": {
      "type": "richtext"
    },
    "description_en": {
      "type": "richtext"
    },
    "technical_info_et": {
      "type": "richtext"
    },
    "technical_info_en": {
      "type": "richtext"
    },
    "raider": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "purchase_description_en": {
      "type": "richtext"
    },
    "remote_id": {
      "type": "uid"
    },
    "producers": {
      "collection": "person"
    },
    "X_artist": {
      "type": "text"
    },
    "X_producer": {
      "type": "string"
    },
    "X_other_works": {
      "type": "string"
    },
    "X_headline_et": {
      "type": "string"
    },
    "X_headline_en": {
      "type": "string"
    },
    "events": {
      "via": "performance",
      "collection": "event"
    },
    "X_town_et": {
      "type": "string"
    },
    "X_town_en": {
      "type": "string"
    },
    "X_pictures": {
      "type": "component",
      "repeatable": false,
      "component": "x-pictures.x-pictures"
    },
    "performance_media": {
      "type": "component",
      "repeatable": true,
      "component": "media.performance-media"
    },
    "coverages": {
      "collection": "coverage",
      "via": "performance"
    },
    "videos": {
      "type": "component",
      "repeatable": true,
      "component": "media.media-content"
    },
    "audios": {
      "type": "component",
      "repeatable": true,
      "component": "media.media-content"
    },
    "duration": {
      "type": "integer"
    },
    "X_premiere_time": {
      "type": "text"
    },
    "users": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "myPerformances"
    },
    "search_field": {
      "type": "string",
      "private": true
    },
    "other_works": {
      "collection": "performance",
      "via": "other_works_back",
      "dominant": true
    },
    "other_works_back": {
      "collection": "performance",
      "via": "other_works"
    },
    "duplicate": {
      "type": "boolean"
    }
  }
}
